<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout android:orientation="vertical"
	android:background="@drawable/bg_set" android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
	<RelativeLayout android:layout_width="fill_parent"
			android:id="@+id/channelRoom_uninit" android:layout_height="fill_parent"
			android:layout_below="@id/channelRoom_title"
			android:layout_marginTop="10dp">
			<TextView android:id="@+id/channelRoom_title"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_marginTop="50dip" android:layout_centerHorizontal="true"
				android:background="@drawable/shenliao_msgroom" android:textColor="#000000" />
			<TextView android:id="@+id/channelRoom_msg1"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_marginTop="20dip" android:layout_below="@id/channelRoom_title"
				android:textSize="12sp" android:textColor="#7599a7"
				android:layout_centerHorizontal="true" android:text="@string/channelRoom_msg_1" />
			<TextView android:id="@+id/channelRoom_msg2"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:layout_marginTop="2dip" android:layout_below="@id/channelRoom_msg1"
				android:textSize="12sp" android:textColor="#7599a7"
				android:layout_centerHorizontal="true" android:text="@string/channelRoom_msg_2" />
	</RelativeLayout>
</LinearLayout>