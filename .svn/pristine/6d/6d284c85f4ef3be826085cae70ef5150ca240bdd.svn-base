<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/search_result_listview_item_bg"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="81dp" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="57dp"
            android:layout_centerVertical="true"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/contact_photo"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:layout_marginLeft="10dp"
                android:src="@drawable/user_infor_head_boy" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/contact_name_ll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/contact_online_btn"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="5.0dip"
                        android:background="@drawable/tuixin_online"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/contact_name"
                        style="@style/IC_Name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="-2dp"
                        android:maxWidth="70.0dip"
                        android:singleLine="true"
                        android:text="name1111111111111111111111111111111"
                        android:textColor="@color/title_text_color"
                        android:textSize="16sp" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/list_item_message_type_rl"
                    android:layout_width="wrap_content"
                    android:layout_height="15dp"
                    android:layout_below="@id/contact_name_ll"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/user_infor_sex_boy" >

                    <TextView
                        android:id="@+id/nearly_choose_tv"
                        android:minWidth="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:text="21"
                       	android:gravity="right"
                        android:textColor="@android:color/white"
                        android:textSize="10.0sp" />
                </RelativeLayout>

                <com.tuixin11sms.tx.view.LevelTextView
                    android:id="@+id/tv_level"
                    style="@style/level_text_style"
                    android:layout_height="15dp"
                    android:layout_alignBottom="@id/list_item_message_type_rl"
                    android:layout_marginLeft="5dp"
                    android:layout_toRightOf="@id/list_item_message_type_rl"
                    android:text="LV22" />

                <TextView
                    android:id="@+id/tx_list_item_sign_str"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="123QW"
                    android:textColor="#777777"
                    android:textSize="12.0sp" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/contact_sig_icn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/sig_pref"
                android:textAppearance="@style/IC_Sig_Pref"
                android:textColor="#b2b4b9"
                android:textSize="15dp"
                android:visibility="gone" />

            <TextView
                android:id="@+id/contact_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:text="123"
                android:textAppearance="@style/IC_Sig_Num"
                android:textColor="#b2b4b9"
                android:textSize="15dp" />
        </LinearLayout>
    </RelativeLayout>

</LinearLayout>